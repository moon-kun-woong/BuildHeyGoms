<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">


<tiles-definitions>

	<definition name="default" template="/WEB-INF/views/board/layout/layout.jsp"> 		
		<put-attribute name="header"  value="/WEB-INF/views/board/layout/header.jsp"/>  
		<put-attribute name="footer"  value="/WEB-INF/views/board/layout/footer.jsp"/>  
		<put-attribute name="side"  value="/WEB-INF/views/board/layout/side.jsp"/>		
	</definition>
	
	<definition name="addBoard" extends="default">
		<put-attribute name="content" value="/WEB-INF/views/board/contents/addBoard.jsp"/>
		<put-attribute name="title" value="addBoard"/> 
	</definition>
	
	<definition name="boardList" extends="default">
		<put-attribute name="content" value="/WEB-INF/views/board/contents/boardList.jsp"/>
		<put-attribute name="title" value="boardList"/> 
	</definition>
	

</tiles-definitions>





